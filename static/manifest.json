{
    "name": "mirage",
    "version": "0.1.0",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "manifest_version": 2,
    "permissions": ["declarativeContent", "storage"],
    "browser_action": {
      "default_icon": {
        "16": "./icons/16.png",
        "32": "./icons/32.png",
        "48": "./icons/48.png",
        "128": "./icons/128.png"
      },
      "default_title": "This is a nonsense tooltip."
    }
}